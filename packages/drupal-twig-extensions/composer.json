{
  "name": "bolt-design-system/drupal-twig-extensions",
  "description": "Extra Twig extensions for adding Drupal-specific functionality to Bolt",
  "version": "2.2.1",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Salem Ghoweri",
      "role": "Maintainer"
    },
    {
      "name": "Evan Lovely"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/drupal/core-render",
      "no-api": true
    }
  ],
  "autoload": {
    "psr-4": {
      "Drupal\\Core\\": "src/Core"
    }
  },
  "require": {
    "twig/twig": "^1.0",
    "drupal/core-render": "^8.0.0",
    "drupal/core-utility": "^8.0.0"
  },
  "scripts": {
    "setup": [
      "@composer install --no-interaction --prefer-dist --no-progress"
    ]
  }
}
